{
  "name": "@anchan828/nest-cloud-run-pubsub-packages",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "husky install",
    "bootstrap": "lerna bootstrap",
    "build": "turbo run build",
    "compodoc": "compodoc -c .compodocrc.json",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "eslint --ignore-pattern packages '**/*.ts' && turbo run lint",
    "lint:fix": "eslint --ignore-pattern packages --fix '**/*.ts' && turbo run lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "turbo run test",
    "new": "ts-node scripts/new-package.ts",
    "postnew": "ts-node scripts/generate-vs-launch.ts",
    "watch": "turbo run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-cloud-run-pubsub.git"
  },
  "author": "anchan828 <anchan828@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchan828/nest-cloud-run-pubsub/issues"
  },
  "homepage": "https://github.com/anchan828/nest-cloud-run-pubsub",
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@commitlint/config-lerna-scopes": "16.2.2",
    "@compodoc/compodoc": "1.1.19",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/testing": "8.4.4",
    "@types/glob": "7.2.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@types/prettier": "2.6.0",
    "@types/rimraf": "3.0.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "glob": "8.0.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "@lerna-lite/cli": "1.0.5",
    "lint-staged": "12.3.8",
    "prettier": "2.6.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "turbo": "1.2.4",
    "typescript": "4.6.3"
  }
}
