{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^copy:license"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "watch": {
      "outputs": []
    },
    "copy:license": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
